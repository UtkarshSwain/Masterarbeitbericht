    \entry{Moreno2019}{article}{}{}
      \name{author}{3}{}{%
        {{hash=402001a3c5cb27080ec4a75467bb09f3}{%
           family={Moreno-garcía},
           familyi={M\bibinithyphendelim g\bibinitperiod},
