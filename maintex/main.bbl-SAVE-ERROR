      \strng{namehash}{072cd84b2c6427e2fe2180bced6aaaf6}
      \strng{fullhash}{072cd84b2c6427e2fe2180bced6aaaf6}
      \strng{fullhashraw}{072cd84b2c6427e2fe2180bced6aaaf6}
      \strng{bibnamehash}{072cd84b2c6427e2fe2180bced6aaaf6}
      \strng{authorbibnamehash}{072cd84b2c6427e2fe2180bced6aaaf6}
